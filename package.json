{
	"name": "electron-vue-starter",
	"productName": "Electron Vue Starter",
	"version": "0.1.0",
	"description": "Convenently build and deploy WP sites on Digital Ocean.",
	"main": "src/index.js",
	"scripts": {
		"start": "electron-forge start --inspect-electron NODE_ENV=development",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"lint": "echo \"No linting configured\"",
		"mix-watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
		"mix-build": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KevinBatdorf/electron-vue-starter.git"
	},
	"keywords": [
		"WordPress",
		"VPS",
		"deployment"
	],
	"author": {
		"name": "Kevin Batdorf",
		"email": "kbat82@gmail.com"
	},
	"license": "GPL-2.0-only",
	"config": {
		"forge": "./forge.config.js"
	},
	"bugs": {
		"url": "https://github.com/KevinBatdorf/electron-vue-starter/issues"
	},
	"homepage": "https://github.com/KevinBatdorf/electron-vue-starter#readme",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.19",
		"@fortawesome/free-brands-svg-icons": "^5.9.0",
		"@fortawesome/free-regular-svg-icons": "^5.9.0",
		"@fortawesome/free-solid-svg-icons": "^5.9.0",
		"@fortawesome/vue-fontawesome": "^0.1.6",
		"@sweetalert2/theme-dark": "^2.0.3",
		"axios": "^0.19.0",
		"electron-reload": "^1.4.1",
		"electron-squirrel-startup": "^1.0.0",
		"electron-store": "^4.0.0",
		"sweetalert2": "^8.14.0",
		"tailwindcss": "^1.0.5",
		"tailwindcss-transition": "^1.0.5",
		"vue": "^2.6.10",
		"vue-router": "^3.0.7",
		"vuex": "^3.1.1"
	},
	"devDependencies": {
		"@electron-forge/cli": "6.0.0-beta.43",
		"@electron-forge/maker-deb": "6.0.0-beta.43",
		"@electron-forge/maker-rpm": "6.0.0-beta.43",
		"@electron-forge/maker-squirrel": "6.0.0-beta.43",
		"@electron-forge/maker-zip": "6.0.0-beta.43",
		"cross-env": "^5.2.0",
		"electron": "9.4.0",
		"laravel-mix": "^4.1.2",
		"path": "^0.12.7",
		"resolve-url-loader": "3.1.0",
		"sass": "^1.22.7",
		"sass-loader": "7.*",
		"vue-devtools": "^5.1.0",
		"vue-template-compiler": "^2.6.10"
	}
}
